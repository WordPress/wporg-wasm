{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/wasm-demo",
  "version": "0.1.0",
  "title": "Wasm Demo",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "wasm-demo",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./front.js",
  "attributes": {
    "step": {
      "type": "string",
      "enum": [
        "settings",
        "playground"
      ]
    },
    "theme": {
      "type": "string",
      "default": ""
    },
    "plugins": {
      "type": "string",
      "default": ""
    }
  }
}